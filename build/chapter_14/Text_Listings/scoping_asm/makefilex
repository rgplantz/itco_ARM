# Build varLife program

objects = varLife.o addConst.o
asmflags = --gstabs

varLife: $(objects)
	gcc $ -o varLife $(objects)

varLife.o: varLife.s
	as $(asmflags) -o varLife.o varLife.s

addConst.o: addConst.s
	as $(asmflags) -o addConst.o addConst.s

.PHONY: clean allclean
clean:
	rm *~ *.o
allclean:
	rm *~ *.o varLife

